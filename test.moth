#include <numoth.moth>
#include <io.moth>
include <time.moth>

@extern
def float sqrtf(float a)

def kernel testKernel[i: 100](float[:] a):
    #print("Hello from thread",i)
    #print(total)
    total = 100.f
    for k in range(1000000):
        total -= sqrtf(total)
    a[i] = total
    pass

def int main():
    myArray = new float[100]
    start = CPUTimer()
    testKernel(myArray)
    end = CPUTimer()
    print(end-start)
    return 0