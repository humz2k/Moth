def int main():
    MPI_Init(NULL,NULL)
    int world_rank
    MPI_Comm_rank(MPI_COMM_WORLD,&world_rank)
    int world_size
    MPI_Comm_size(MPI_COMM_WORLD,&world_size)
    if world_rank == 0:
        print("RANK:",world_rank)
        print("SIZE:",world_size)
    ndims = 3
    dims = new int[3]
    MPI_Dims_create(world_size,ndims,int*(dims))
    if world_rank == 0:
        print("DIMS: ",__nonewln)
        for i in dims:
            print(i,"",__nonewln)
        print()
    MPI_Finalize()
    return 0