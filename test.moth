@template
class NDArray[T,d]:
    T* raw
    int dim
    int size
    tuple[int] shape

    def void __init__(NDArray[T,d] self, tuple[int] _shape):
        self.dim = d
        self.shape = _shape
        if self.dim != 0:
            if self.dim != self.shape._len:
                print("Incorrect Dimensions")
                exit(1)
        self.size = 1
        
        
        

def int main():
    int[:] test = new NDArray[int,1]((1,))
    return 0